{
    "breadcrumbs.enabled": true,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.rulers": [80, 120],
    "explorer.autoReveal": true,
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": true,
    "files.associations": {
        "Dockerfile-*": "dockerfile",
    },
    "files.exclude": {
        "**/.git/": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/CVS": true,
    },
    "files.insertFinalNewline": true,
    "go.formatTool": "goimports",
    "search.exclude": {
        "**/bower_components": true,
        "**/node_modules": true,
        "**/target": true
    },
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "[Monokai]": {
            // AtomOne Light sidebar
            "editor.background": "#2c2d26",
            "editorError.foreground": "#e65544",
            "editorIndentGuide.background": "#41413d",
            "editorIndentGuide.activeBackground": "#6c6c66",
            "editorRuler.foreground": "#41413d",
            "editorSuggestWidget.background": "#2c2d26",
            "editorWarning.foreground": "#e65544",
            "editorWidget.background": "#2c2d26",
            "gitDecoration.addedResourceForeground": "#4ca24a",
            "gitDecoration.conflictingResourceForeground": "#a726a5",
            "gitDecoration.deletedResourceForeground": "#e65544",
            "gitDecoration.modifiedResourceForeground": "#cdaa64",
            "gitDecoration.untrackedResourceForeground": "#4ca24a",
            "list.activeSelectionBackground": "#dbdbdc",
            "list.activeSelectionForeground": "#232324",
            "list.dropBackground": "#dbdde0",
            "list.errorForeground": "#e65544",
            "list.hoverBackground": "#dbdbdc",
            "list.hoverForeground": "#383a42",
            "list.inactiveSelectionBackground": "#dbdbdc",
            "list.inactiveSelectionForeground": "#232324",
            "list.focusBackground": "#dbdbdc",
            "list.focusForeground": "#232324",
            "list.highlightForeground": "#329cbe",
            "list.warningForeground": "#e65544",
            "sideBar.background": "#eaeaeb",
            "sideBar.foreground": "#383a42",
            "sideBarSectionHeader.background": "#fafafa",
            "statusBar.background": "#2c2d26",
            "statusBar.border": "#41413d",
            "statusBar.foreground": "#90908a",
            "titleBar.activeBackground": "#eaeaeb",
            "titleBar.activeForeground": "#383a42",
            "titleBar.inactiveBackground": "#eaeaeb",
            "titleBar.inactiveForeground": "#383a42"
        },
    },
    "workbench.colorTheme": "Monokai",
    "workbench.editor.limit.enabled": true,
    "workbench.editor.showTabs": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.visible": true,
    "workbench.tips.enabled": false,
    "yaml.validate": false,
}
