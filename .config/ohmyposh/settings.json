{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
        "blue": "#8aadf4",
        "pink": "#c6a0f6",
        "text": "#363a4f"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:pink",
          "foreground": "p:text",
          "properties": {
            "home_icon": "~",
            "style": "folder"
          },
          "trailing_diamond": "\ue0b0",
          "style": "diamond",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:blue",
          "foreground": "p:text",
          "style": "powerline",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": false,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "powerline_symbol": "\ue0b0",
          "template": " {{ .HEAD }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "template": "<,p:text> </><p:text,transparent>\ue0b0</> ",
    "background": "transparent",
    "foreground": "transparent"
  },
  "final_space": true,
  "version": 3
}
